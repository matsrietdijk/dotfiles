[include]
    path = ~/.gitconfig_private
[credential]
    helper = osxkeychain
[push]
    default = simple
[alias]
    ap = add -p
    br = branch
    ci = commit
    cl = clone
    co = checkout
    cp = cherry-pick
    dc = diff --cached
    di = diff
    ds = diff --staged
    fe = fetch
    hash = rev-parse --verify
    hs = hash
    lg = log --graph --pretty=format:'%C(auto)%h %d %s%C(reset) %C(cyan)[%an]%C(reset)'
    mr = merge
    o = browse
    pl = pull
    ps = push
    pso = push -u origin
    rb = rebase
    rbc = rebase --continue
    sh = hash --short
    st = status -s
[pull]
    rebase = true
[rerere]
    enabled = true
